<h3>Update Client's Name</h3>
  <form action="/clients/<%= @client.id() %>" method="post">
    <input name="_method" type="hidden" value="patch">
    <label for="new-client-name">Update Name:</label>
    <input id="new-client-name" name="new-client-name" type="text" placeholder="<%= @client.name() %>" required>
  </form>
